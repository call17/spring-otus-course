<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="consoleService" class="ru.otus.homework1.service.impl.ConsoleServiceImpl"/>
    <bean id="csvDao" class="ru.otus.homework1.dao.CsvDao"/>
    <bean id="csvService" class="ru.otus.homework1.service.impl.CsvServiceImpl">
        <constructor-arg ref="csvDao"/>
    </bean>
    <bean id="testService" class="ru.otus.homework1.service.impl.TestServiceImpl">
        <constructor-arg ref="consoleService"/>
        <constructor-arg ref="csvService" />
    </bean>
</beans>
